package Arrays;

public class Arrayexample {

	public static void main(String[] args)
	{
		//datatype Arraname[] = new datatype[size]
// Array index always starts with 0		
		int a[]= new int[3];
		
		a[0] = 10;
		a[1] = 20;
		a[2] = 30;
		a[3] =40;
		
	for ( int i =0; i<a.length;i++)
	{
		
		System.out.println(a[i]);
	}	
		

	}

}
